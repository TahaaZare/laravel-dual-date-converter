{
    "name": "tahaazare/dual-date-converter",
    "description": "Convert Jalali <-> Gregorian dates in Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "morilog/jalali": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TahaZare\\DualDateConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TahaaZare",
            "email": "tahaazre.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TahaaZare\\DualDateConverter\\DualDateConverterServiceProvider"
            ]
        }
    }
}
